<% content_for(:head) do %>
  <%= rel_next_prev_link_tags(@results) %>
<% end %>

<div class="h-feed">
  <%= render "shared/site_search", location: :top %>

  <header>
    <h1>Search results for “<%= @search.query %>”</h1>
  </header>

  <div class="articles-list">
    <ol>
      <% @results.each do |result| %>
        <%= render "result", result: result %>
      <% end %>
    </ol>
  </div>
</div>

<%= paginate @results %>
